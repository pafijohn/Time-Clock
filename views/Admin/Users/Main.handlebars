<script>
$(document).ready(
	function() {
		handleClick = function(id) {
			window.location.href = '/Admin/Users/Edit/' + id;
		}
		$('nav>section>ul>li>a[href="/Admin/Users"]').parent().addClass('active')
	}
);
</script>

<table width=100%>
	<thead>
		<tr>
			<th width="50%">User&nbsp;</th>
		</tr>
	</thead>
	<tbody>
			<tr class="pseudo-link" onclick="handleClick('')">
				<td>Create</td>
			</tr>
		{{#each rows}}
			<tr class="pseudo-link" onclick="handleClick({{this._id}})">
				<td>{{this.Name}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>